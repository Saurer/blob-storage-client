{
    "env": {
        "development": {
            "presets": [
                "next/babel",
                [
                    "@emotion/babel-preset-css-prop",
                    {
                        "sourceMap": true
                    }
                ]
            ],
            "plugins": [
                "@babel/plugin-proposal-optional-chaining"
            ]
        },
        "production": {
            "presets": [
                "next/babel",
                "@emotion/babel-preset-css-prop"
            ],
            "plugins": [
                "@babel/plugin-proposal-optional-chaining"
            ]
        },
        "test": {
            "presets": [
                [
                    "next/babel",
                    {
                        "preset-env": {
                            "modules": "commonjs"
                        }
                    }
                ],
                "@emotion/babel-preset-css-prop"
            ],
            "plugins": [
                "@babel/plugin-proposal-optional-chaining"
            ]
        }
    }
}